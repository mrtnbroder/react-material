{
  "name": "react-material",
  "version": "0.1.0",
  "description": "Material Design React Components",
  "main": "dist/lib/index",
  "module": "dist/esm/index",
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm run clean && npm run build:cjs & npm run build:esm & npm run build:umd & npm run build:min",
    "build:cjs": "BABEL_ENV=cjs babel ./src -d dist/lib --ignore '__tests__'",
    "build:esm": "BABEL_ENV=esm babel ./src -d dist/esm --ignore '__tests__'",
    "build:min": "NODE_ENV=production webpack -p src/index.js dist/umd/react-material.min.js",
    "build:umd": "NODE_ENV=development webpack src/index.js dist/umd/react-material.js",
    "build:watch": "npm run build:esm -- --watch",
    "build:docs": "documentation serve ./src/index.js -f md",
    "build:docs:watch": "npm run build:docs -- --watch",
    "ci": "npm run test && npm run typecheck",
    "clean": "rm -rf ./dist",
    "coverage": "jest --coverage && coveralls < coverage/lcov.info",
    "examples": "cd examples && npm start",
    "flow-typed": "flow-typed update",
    "lint": "npm run lint:css",
    "lint:css": "stylelint './packages/**/*.js'",
    "prepublish": "npm run build",
    "typecheck": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrtnbroder/react-material.git"
  },
  "keywords": [
    "react",
    "material",
    "design",
    "mdl",
    "google",
    "light",
    "dark",
    "theme",
    "universal",
    "isomorphic"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "Martin Broder <hello@martinbroder.com> (https://martinbroder.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrtnbroder/react-material/issues"
  },
  "homepage": "https://github.com/mrtnbroder/react-material#readme",
  "dependencies": {
    "color": "3.0.0",
    "dom-helpers": "3.3.1",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-motion": "0.5.2",
    "recompose": "0.26.0",
    "styled-components": "3.1.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-flow-comments": "6.3.19",
    "babel-plugin-styled-components": "1.5.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "coveralls": "3.0.0",
    "documentation": "5.3.5",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.16.0",
    "flow-bin": "0.64.0",
    "flow-typed": "2.3.0",
    "jest": "22.1.4",
    "jest-enzyme": "4.1.1",
    "lerna": "^2.8.0",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.2.2",
    "webpack": "3.10.0"
  },
  "files": [
    "*.md",
    "dist"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "IE 10"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "setupTestFrameworkScriptFile": "./src/setupTests.js"
  }
}
